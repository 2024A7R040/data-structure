#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node *next;

} node;
node* createNode(int data){
    node* newNode = (node*)malloc(sizeof(node));
    newNode->data = data;
    newNode->next = NULL;
    return newNode;
}
void insertNodeInBeg(int ele){
    node* newNode = createNode(ele);
